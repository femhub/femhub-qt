#!/usr/bin/env python

import sys
from PySide.QtCore import QUrl
from PySide.QtGui import QApplication
from PySide.QtWebKit import QWebView

app = QApplication(sys.argv)

web = QWebView()
web.load(QUrl("http://lab.femhub.org/"))
web.setWindowTitle("FEMhub Desktop GUI")
web.show()

sys.exit(app.exec_())
